      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        Film.Title, Film.ReleaseDate, Film.DirectorID, Film.BudgetDollars, Film.BoxOfficeDollars, Director.FullName
FROM            Film INNER JOIN
                         Director ON Film.DirectorID = Director.DirectorID
